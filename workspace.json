{
  "version": 2,
  "projects": {
    "be-products": {
      "root": "apps/be/products",
      "sourceRoot": "apps/be/products/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/be/products",
            "main": "apps/be/products/src/main.ts",
            "tsConfig": "apps/be/products/tsconfig.app.json",
            "assets": ["apps/be/products/src/assets"],
            "generatePackageJson": true
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/be/products/src/environments/environment.ts",
                  "with": "apps/be/products/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "be-products:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/be/products/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/be/products"],
          "options": {
            "jestConfig": "apps/be/products/jest.config.js",
            "passWithNoTests": true
          }
        },
        "docker": {
          "executor": "@nx-tools/nx-docker:build",
          "options": {
            "file": "apps/be/products/Dockerfile",
            "push": true,
            "tags": [
              "eu.gcr.io/mussia8/be-products:latest",
              "eu.gcr.io/mussia8/be-products:v1"
            ]
          }
        },
        "cloud": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [],
          "options": {
            "command": "gcloud run deploy be-products --image eu.gcr.io/mussia8/be-products --platform managed  --allow-unauthenticated  --region europe-west1 --port 3333"
          }
        }
      }
    },
    "be-users": {
      "root": "apps/be/users",
      "sourceRoot": "apps/be/users/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/be/users",
            "main": "apps/be/users/src/main.ts",
            "tsConfig": "apps/be/users/tsconfig.app.json",
            "assets": ["apps/be/users/src/assets"],
            "generatePackageJson": true
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/be/users/src/environments/environment.ts",
                  "with": "apps/be/users/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "be-users:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/be/users/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/be/users"],
          "options": {
            "jestConfig": "apps/be/users/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "big-data-func1": "apps/big-data/func1",
    "button": {
      "root": "libs/button",
      "sourceRoot": "libs/button/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/button",
            "tsConfig": "libs/button/tsconfig.lib.json",
            "project": "libs/button/package.json",
            "entryFile": "libs/button/src/index.ts",
            "external": ["react/jsx-runtime"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/button/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/button/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/button"],
          "options": {
            "jestConfig": "libs/button/jest.config.js",
            "passWithNoTests": true
          }
        },
        "deploy": {
          "executor": "ngx-deploy-npm:deploy",
          "options": {
            "access": "public"
          }
        },
        "version": {
          "executor": "@jscutlery/semver:version",
          "options": {
            "syncVersions": false
          }
        }
      }
    },
    "fe-react-app": {
      "root": "apps/fe/react-app",
      "sourceRoot": "apps/fe/react-app/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/fe/react-app",
            "index": "apps/fe/react-app/src/index.html",
            "main": "apps/fe/react-app/src/main.tsx",
            "polyfills": "apps/fe/react-app/src/polyfills.ts",
            "tsConfig": "apps/fe/react-app/tsconfig.app.json",
            "assets": [
              "apps/fe/react-app/src/favicon.ico",
              "apps/fe/react-app/src/assets"
            ],
            "styles": ["apps/fe/react-app/src/styles.css"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/fe/react-app/src/environments/environment.ts",
                  "with": "apps/fe/react-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "fe-react-app:build",
            "hmr": true,
            "port": 3000,
            "open": true,
            "proxyConfig": "apps/fe/react-app/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "fe-react-app:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/fe/react-app/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/fe/react-app"],
          "options": {
            "jestConfig": "apps/fe/react-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "fe-react-app-e2e": {
      "root": "apps/fe/react-app-e2e",
      "sourceRoot": "apps/fe/react-app-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/fe/react-app-e2e/cypress.json",
            "devServerTarget": "fe-react-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "fe-react-app:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/fe/react-app-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "shared-data-types": {
      "root": "libs/shared/data-types",
      "sourceRoot": "libs/shared/data-types/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shared/data-types/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/data-types"],
          "options": {
            "jestConfig": "libs/shared/data-types/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      }
    },
    "@nrwl/web:application": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/web:library": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "fe-react-app"
}
